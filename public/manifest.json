{
  "manifest_version": 2,
  "name": "meguro",
  "description": "meguro online bookmark",
  "version": "0.0.1",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "icon48.png",
    "48": "icon48.png",
    "128": "icon192.png"
  },
  "content_security_policy": "script-src 'self' 'sha256-GgRxr...'; object-src 'self'",
  "permissions": [
    "activeTab"
  ]
}
